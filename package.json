{
  "name": "@brixel/ui-task-sdk",
  "version": "1.0.0",
  "description": "SDK for building Brixel UI Tasks - interactive React components that integrate with Brixel workflows",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external react",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist dist-tarballs",
    "prepack": "npm run build",
    "pack:local": "mkdir -p dist-tarballs && npm pack --pack-destination dist-tarballs",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "react": "^19.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "brixel",
    "ui-task",
    "workflow",
    "react",
    "iframe",
    "postmessage"
  ],
  "author": "Brixel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brixel-ai/ui-task-sdk"
  }
}
